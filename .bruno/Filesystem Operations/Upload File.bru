meta {
  name: Upload File
  type: http
  seq: 4
}

post {
  url: {{baseurl}}/server/:server-id/fs/upload
  body: multipartForm
  auth: inherit
}

params:path {
  server-id:
}

params:query {
  upload_id: {{$guid}}
  path: /example-file.jar
}

body:multipart-form {
  file: @file(/path/to/local/file.jar)
}
